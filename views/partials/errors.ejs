
         <% if(errors.length>0) {%>
            <div class="bg-red-100 border border-red-400 text-red-700 px-4 py-3 rounded relative mb-4 w-full" role="alert">
              <strong class="font-bold">Error!</strong>
              <ul class="list-disc list-inside">
                <% errors.forEach(function(error) { %>
                  <li><%= error %></li>
                <% }); %>
              </ul>
            </div>
          <% } %> 